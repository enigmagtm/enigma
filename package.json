{
  "name": "enigma-srcs",
  "version": "1.0.0",
  "description": "Enigma Framework for Node server aplications",
  "scripts": {
    "lint": "eslint packages/**/*.ts --fix",
    "test": "jest",
    "install-cli": "cd packages/cli && npm run install-pkg",
    "link-cli": "cd packages/cli && npm run link",
    "unlink-cli": "cd packages/cli && npm run unlink",
    "build-cli": "cd packages/cli && npm run build",
    "publish-cli": "cd packages/cli && npm run publish-pkg",
    "outdated-cli": "cd packages/cli && npm outdated",
    "pack-core": "cd packages/core && npm run pack",
    "install-core": "cd packages/core && npm run install-pkg",
    "build-core": "cd packages/core && npm run build",
    "publish-core": "cd packages/core && npm run publish-pkg",
    "pack-orm": "cd packages/orm && npm pack",
    "install-orm": "cd packages/orm && npm run install-pkg",
    "build-orm": "cd packages/orm && npm run build",
    "publish-orm": "cd packages/orm && npm run publish-pkg",
    "pack-http": "cd packages/http && npm pack",
    "install-http": "cd packages/http && npm run install-pkg",
    "build-http": "cd packages/http && npm run build",
    "publish-http": "cd packages/http && npm run publish-pkg",
    "pack-rsc": "cd packages/rsc && npm pack",
    "install-rsc": "cd packages/rsc && npm run install-pkg",
    "build-rsc": "cd packages/rsc && npm run build",
    "publish-rsc": "cd packages/rsc && npm run publish-pkg",
    "install-all": "npm run install-core && npm run install-orm && npm run install-http && npm run install-rsc && npm run install-cli",
    "build-all": "npm run build-core && npm run build-orm && npm run build-http && npm run build-rsc && npm run build-cli",
    "pack-all": "npm run pack-core && npm run pack-orm && npm run pack-http && npm run pack-rsc && npm run pack-cli",
    "full-install": "npm run install-all && npm run build-all && npm run pack-all",
    "publish_all": "npm run publish-core && npm run publish-orm && npm run publish-http && npm run publish-rsc && npm run publish-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enigmagtm/enigma.git"
  },
  "keywords": [
    "enigma"
  ],
  "author": "snakeful",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enigmagtm/enigma/issues"
  },
  "homepage": "https://github.com/enigmagtm/enigma#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.177",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  }
}