{
  "name": "@enigmagtm/core",
  "version": "0.0.0",
  "description": "Enigma core framework",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && cp {*.md,package.json} dist",
    "install:pkg": "npm i --no-save --legacy-peer-deps && npm-install-peers",
    "publish:pkg": "npm run build && cd dist && npm publish --access public",
    "version:pkg": "npm version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enigmagtm/enigma.git",
    "directory": "packages/core"
  },
  "keywords": [
    "ngm",
    "enigma",
    "core"
  ],
  "license": "MIT",
  "peerDependencies": {
    "express": "4.17.3",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.182"
  }
}