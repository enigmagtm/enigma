{
  "name": "@enigmagtm/core",
  "version": "1.0.0",
  "description": "Enigma core framework",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "install-pkg": "npm i --no-save && npm-install-peers",
    "build": "rm -rf dist && tsc -p tsconfig.json && cp {README.md,package.json,LICENSE.md} dist",
    "publish-pkg": "npm run build && cd dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enigmagtm/enigma.git",
    "directory": "packages/core"
  },
  "keywords": [
    "ngm",
    "enigma",
    "core"
  ],
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.177"
  }
}
