{
  "name": "@enigmagtm/cli",
  "version": "0.0.0",
  "description": "Enigma command line tool",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && cp LICENSE README.md package.json ./dist && mkdir -p ./dist/src/assets && cp src/assets/*.* ./dist/src/assets",
    "install:pkg": "npm i --no-save",
    "link": "npm run build && npm link",
    "publish:pkg": "npm run build && cd dist && npm publish --access public",
    "unlink": "npm unlink",
    "version:pkg": "npm version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enigmagtm/enigma.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "ngm",
    "enigma",
    "cli"
  ],
  "license": "MIT",
  "bin": {
    "ngm": "./dist/src/cli.js"
  },
  "dependencies": {
    "colors": "1.4.0",
    "commander": "9.2.0",
    "knex": "2.0.0",
    "lodash": "4.17.21",
    "prompt-sync": "4.2.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/prompt-sync": "4.1.1"
  }
}