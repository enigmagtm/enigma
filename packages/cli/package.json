{
  "name": "@enigmagtm/cli",
  "version": "1.2.0",
  "description": "Enigma command line tool",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && cp {*.md,package.json} dist",
    "install:pkg": "npm i --no-save",
    "link": "npm run build && npm link",
    "publish:pkg": "npm run build && cd dist && npm publish --access public",
    "unlink": "npm unlink",
    "version:pkg": "npm version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enigmagtm/enigma.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "ngm",
    "enigma",
    "cli"
  ],
  "license": "MIT",
  "bin": {
    "ngm": "./dist/src/cli.js"
  },
  "dependencies": {
    "knex": "^0.95.14",
    "lodash": "^4.17.21",
    "prompt-sync": "^4.2.0"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.177",
    "@types/prompt-sync": "^4.1.1"
  }
}
